{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".tsbuild",
    "paths": {
      "@/*": [
        "*"
      ],
      "@karmaniverous/smoz": [
        "../../.stan/dist/index.d.ts",
        "../../dist/index.d.ts"
      ]
    },
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [
