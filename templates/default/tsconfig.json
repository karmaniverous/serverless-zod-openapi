{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".tsbuild",
    "paths": {
      "@/*": ["*"],
      "@karmaniverous/smoz": [
        "../../dist/index",
        "../../dist/index.d.ts"
      ]
    },
    "typeRoots": [
      "./types",
      "../../node_modules/@types"
    ],
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [
    ".serverless/**",
    "node_modules/**",
    "app/generated/**",
    "dist/**"
  ],
  "extends": "./tsconfig.base.json",
  "include": ["**/*", "**/*.json"]
}