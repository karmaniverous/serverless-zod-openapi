{
  "entry": ["serverless.ts", "services/activecampaign/src/index.ts"],
  "project": [
    "src/**",
    "app/**",
    "services/**",
    "!**/generated/**",
    "!docs/**"
  ],
  "ignore": [
    "lib/test/**",
    "app/**/handler.ts",
    "app/functions/**/lambda.ts",
    "app/functions/**/openapi.ts",
    "src/serverless/intrinsic.ts",
    "services/**/src/openapi.json",
    "services/activecampaign/orval.config.ts",
    "services/activecampaign/src/wrapped/field-values.ts",
    "services/activecampaign/src/api/contacts/format.ts"
  ],
  "ignoreBinaries": ["plantuml"],
  "ignoreDependencies": [
    "http-errors",
    "@types/http-errors",
    "@types/serverless",
    "@vitest/coverage-v8",
    "serverless-apigateway-log-retention",
    "serverless-deployment-bucket",
    "serverless-domain-manager",
    "serverless-plugin-common-excludes",
    "serverless-offline"
  ],
  "rules": { "exports": "off", "types": "off" }
}
