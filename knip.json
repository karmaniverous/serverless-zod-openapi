{
  "workspaces": {
    ".": {
      "entry": ["serverless.ts"],
      "project": ["src/**", "app/**", "!docs/**"],
      "ignore": ["lib/test/**", "app/**/handler.ts"]
    },
    "services/activecampaign": {
      "entry": ["src/index.ts"],
      "project": ["src/**", "!**/generated/**"],
      "ignore": ["**/generated/**", "src/openapi.json"]
    }
  },
  "ignoreDependencies": [
    "@karmaniverous/cached-axios",
    "http-errors",
    "@types/http-errors",
    "@types/serverless",
    "@vitest/coverage-v8",
    "serverless-apigateway-log-retention",
    "serverless-deployment-bucket",
    "serverless-domain-manager",
    "serverless-plugin-common-excludes"
  ],
  "rules": { "exports": "off", "types": "off" }
}
