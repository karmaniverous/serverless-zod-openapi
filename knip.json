{
  "entry": ["serverless.ts"],
  "project": ["src/**", "app/**", "!**/generated/**", "!docs/**"],
  "ignore": [
    "lib/test/**",
    "app/**/handler.ts",
    "app/functions/**/lambda.ts",
    "app/functions/**/openapi.ts",
    "src/serverless/intrinsic.ts",
    "src/serverless/plugin.ts",
    "src/cli/local/inline.server/**"
  ],
  "ignoreBinaries": ["plantuml"],
  "ignoreDependencies": [    "http-errors",
    "@types/http-errors",
    "@types/serverless",
    "@vitest/coverage-v8",
    "serverless-apigateway-log-retention",
    "serverless-deployment-bucket",
    "serverless-domain-manager",
    "serverless-plugin-common-excludes",
    "serverless-offline"
  ],
  "rules": { "exports": "off", "types": "off" }
}
