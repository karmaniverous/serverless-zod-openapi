{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "noEmit": false,
    "sourceMap": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@@/*": ["services/activecampaign/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "stack/**/*.ts",
    "stack/**/*.json"
  ],  "exclude": [
    "node_modules/**",
    ".tsbuild/**"
  ]
}