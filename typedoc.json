{
  "categoryOrder": [
    "Public API",
    "HTTP Middleware",
    "Customization",
    "Config",
    "Serverless",
    "OpenAPI",
    "Types"
  ],
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "exclude": ["**/*.test.ts", "src/test/**", "**/generated/**", "**/*.d.ts"],
  "excludeInternal": false,
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "md",
    "tsx",
    "typescript"
  ],
  "hostedBaseUrl": "https://docs.karmanivero.us/smoz",
  "navigation": {
    "includeCategories": true,
    "includeFolders": false
  },
  "navigationLinks": {
    "GitHub": "https://github.com/karmaniverous/smoz"
  },
  "out": "./docs/",
  "plugin": [
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-replace-text",
    "typedoc-plugin-zod"
  ],
  "projectDocuments": ["./CHANGELOG.md"],
  "replaceText": {
    "replacements": [
      {
        "flags": "gis",
        "pattern": "<!-- TYPEDOC_EXCLUDE -->.*?<!-- /TYPEDOC_EXCLUDE -->",
        "replace": ""
      }
    ]
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "sort": ["alphabetical"]
}
