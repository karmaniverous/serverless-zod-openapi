{
  "compilerOptions": {
    "baseUrl": ".",
    "declarationDir": ".tsbuild",
    "outDir": ".tsbuild",
    "noEmit": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "declarationMap": false,
    "paths": {
      "@/*": ["*"],
      "@@/*": ["services/activecampaign/*"]
    },
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [".serverless/**", "node_modules/**"],
  "extends": "./tsconfig.base.json",
  "include": ["**/*", "**/*.json"]
}