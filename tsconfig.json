{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".tsbuild",
    "paths": {
      "@/*": ["*"]
    },
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [
    ".serverless/**",
    "node_modules/**",
    "app/generated/**",
    "templates/**"
  ],
  "extends": "./tsconfig.base.json",
  "include": ["**/*", "**/*.json"]
}
