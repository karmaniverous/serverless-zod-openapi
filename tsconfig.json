{
  "compilerOptions": {
    "baseUrl": ".",
    "declarationDir": ".tsbuild",
    "outDir": ".tsbuild",
    "paths": {
      "@@/*": ["*"],
      "@/*": ["services/activecampaign/*", "packages/cached-axios/*"]
    },
    "tsBuildInfoFile": ".tsbuild/tsconfig.tsbuildinfo"
  },
  "exclude": [".serverless/**", "node_modules/**", "tools/context/out/**"],
  "extends": "./tsconfig.base.json",
  "include": ["**/*", "**/*.json"]
}
